<% if user_signed_in? %>
<table  class="table table-hover">
<tr>
	<th>花費</th>
	<th>種類</th>
	<th>新增時間</th>
</tr>	
<%@item.each do |i|%>
<tr>
	<td><%=i.cost%></td>
	<td><%=i.kind%></td>
	<td><%=i.created_at%> </td>
</tr>
<%end%>
</table>
<%= paginate @item %>
<% else %>
<h1>使用前請先<%= link_to "登入", new_user_session_path %></h1>
<% end %>
